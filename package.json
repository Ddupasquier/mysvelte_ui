{
  "name": "mysvelte-ui",
  "version": "0.0.38",
  "license": "MIT",
  "engines": {
    "node": ">=20.19.0"
  },
  "files": [
    "dist/*"
  ],
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && npm run package",
    "preview": "vite preview",
    "package": "svelte-kit sync && svelte-package && publint",
    "prepublishOnly": "npm run package && publint",
    "test": "playwright test",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test:unit": "vitest",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "newcomp": "node scripts/newComponent.js",
    "cleandoc": "node scripts/cleanDocs.js src/routes/components",
    "filesearch": "node scripts/fileSearch.js",
    "createdoc": "node scripts/create_doc.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "peerDependencies": {
    "svelte": "^5.0.0"
  },
  "types": "./dist/index.d.ts",
  "svelte": "./dist/index.js",
  "devDependencies": {
    "@fontsource/fira-mono": "^5.2.7",
    "@playwright/test": "^1.56.1",
    "@sveltejs/adapter-auto": "^7.0.0",
    "@sveltejs/kit": "^2.49.0",
    "@sveltejs/package": "^2.5.6",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@types/pluralize": "^0.0.33",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "eslint": "^9.39.1",
    "eslint-plugin-storybook": "^10.0.8",
    "eslint-plugin-svelte": "^3.13.0",
    "mdsvex": "^0.12.6",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "publint": "^0.3.15",
    "sass": "^1.94.2",
    "svelte": "^5.43.14",
    "svelte-check": "^4.3.4",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "vite": "^7.2.4",
    "vitest": "^4.0.13"
  },
  "type": "module",
  "dependencies": {
    "@types/gtag.js": "^0.0.20",
    "pluralize": "^8.0.0"
  }
}
