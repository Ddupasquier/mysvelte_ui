{
  "name": "mysvelte-ui",
  "version": "0.0.25",
  "license": "MIT",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && npm run package",
    "preview": "vite preview",
    "package": "svelte-kit sync && svelte-package && publint",
    "prepublishOnly": "npm run package && publint",
    "test": "playwright test",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test:unit": "vitest",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "newcomp": "node scripts/newComponent.js",
    "cleandoc": "node scripts/cleanDocs.js src/routes/components",
    "filesearch": "node scripts/fileSearch.js src/routes/components",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "peerDependencies": {
    "svelte": "^3.54.0"
  },
  "types": "./dist/index.d.ts",
  "svelte": "./dist/index.js",
  "devDependencies": {
    "@fontsource/fira-mono": "^4.5.10",
    "@neoconfetti/svelte": "^1.0.0",
    "@storybook/addon-a11y": "^7.0.2",
    "@storybook/addon-docs": "^7.0.2",
    "@storybook/addon-essentials": "^7.0.2",
    "@storybook/addon-interactions": "^7.0.2",
    "@storybook/addon-links": "^7.0.2",
    "@storybook/addon-svelte-csf": "^3.0.0",
    "@storybook/blocks": "^7.0.2",
    "@storybook/manager-api": "^7.0.2",
    "@storybook/svelte": "^7.0.2",
    "@storybook/sveltekit": "^7.0.2",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@storybook/theming": "^7.0.2",
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/kit": "^1.5.0",
    "@sveltejs/package": "^2.0.1",
    "@types/cookie": "^0.5.1",
    "@types/pluralize": "^0.0.29",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^8.28.0",
    "eslint-plugin-storybook": "^0.6.11",
    "eslint-plugin-svelte3": "^4.0.0",
    "fs": "^0.0.1-security",
    "mdsvex": "^0.10.6",
    "publint": "^0.1.9",
    "sass": "^1.58.1",
    "storybook": "^7.0.2",
    "svelte": "^3.54.0",
    "svelte-check": "^3.0.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.3",
    "vite": "^4.0.0",
    "pluralize": "^8.0.0"
  },
  "type": "module",
  "dependencies": {
    "@types/gtag.js": "^0.0.12"
  }
}
